<div class="random-container"> 
    <h1>Random Beer</h1>
    <div id="group">
        <img src="{{this.image_url}}" alt="Imagen de la cerveza">
        <div>
            <h2>{{this.name}}</h2>
            <h3>{{this.tagline}}</h3>
            <p>{{this.description}}</p>
            <h5> Food Pairing</h5>
            <ul>
                {{#each this.food_pairing}}
                <li>{{this}}</li>
                {{/each}}
            </ul>
            <p>Brewer Tips: {{this.brewers_tips}}</p>
        </div>
    </div>
</div>

  
